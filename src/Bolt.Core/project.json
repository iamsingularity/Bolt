{
  "version": "0.40.0-alpha1",
  "authors": [
    "Martin Tomka"
  ],
  "description": "Bolt Core library containing common classes shared by client and server.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/justkao/bolt"
    },
    "tags": [
      "bolt",
      "core"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true
  },
  "dependencies": {
    "Newtonsoft.Json": "8.0.4-beta1"
  },
  "frameworks": {
    "netstandard1.1": {
      "dependencies": {
        "System.Collections": "4.0.11-*",
        "System.Collections.Concurrent": "4.0.12-*",
        "System.Linq": "4.1.0-*",
        "System.Net.Http": "4.0.1-*",
        "System.Reflection": "4.1.0-*",
        "System.Reflection.Extensions": "4.0.1-*",
        "System.Threading": "4.0.11-*"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime.Serialization": { "type": "build" },
        "System.Net.Http": { "type": "build" }
      }
    }
  }
}